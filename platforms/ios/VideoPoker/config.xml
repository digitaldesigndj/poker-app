<?xml version='1.0' encoding='utf-8'?>
<widget id="com.hyprtxt.poker" version="0.8.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <icon height="60" src="www/res/ios/poker-60.png" width="60" />
    <icon height="120" src="www/res/ios/poker-60@2x.png" width="120" />
    <icon height="76" src="www/res/ios/poker-76.png" width="76" />
    <icon height="152" src="www/res/ios/poker-76@2x.png" width="152" />
    <icon height="40" src="www/res/ios/poker-40.png" width="40" />
    <icon height="80" src="www/res/ios/poker-40@2x.png" width="80" />
    <icon height="57" src="www/res/ios/poker.png" width="57" />
    <icon height="114" src="www/res/ios/poker@2x.png" width="114" />
    <icon height="72" src="www/res/ios/poker-72.png" width="72" />
    <icon height="144" src="www/res/ios/poker-72@2x.png" width="144" />
    <icon height="29" src="www/res/ios/poker-small.png" width="29" />
    <icon height="58" src="www/res/ios/poker-small@2x.png" width="58" />
    <icon height="50" src="www/res/ios/poker-50.png" width="50" />
    <icon height="100" src="www/res/ios/poker-50@2x.png" width="100" />
    <name>VideoPoker</name>
    <description>
		JS Video Poker
	</description>
    <author email="taylor@hyprtxt.com" href="http://hyprtxt.com">
		Hyprtxt
	</author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="icons/icon.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="landscape" />
    <preference name="BackgroundColor" value="0xff0000ff" />
</widget>
